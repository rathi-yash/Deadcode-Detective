{
  "name": "deadcode-detective",
  "version": "1.0.1",
  "description": "A tool to detect dead code in JavaScript/TypeScript and Python projects",
  "main": "dist/cli.js",
  "bin": {
    "deadcode-detective": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "node dist/cli.js detect --js ./test-project/js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "deadcode",
    "javascript",
    "typescript",
    "python",
    "code-quality",
    "cli"
  ],
  "author": "Yash",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rathi-yash/Deadcode-Detective.git"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^11.0.1",
    "ora": "^8.0.1",
    "ts-prune": "^0.10.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.7.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.6.3"
  }
}
